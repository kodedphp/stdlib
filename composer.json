{
  "name": "koded/stdlib",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "A standard library for shareable classes and functions",
  "keywords": ["mime", "mime-types", "uuid", "uuid-generator", "immutable", "utility", "exceptions", "dto", "dot-array", "php7"],
  "authors": [
    {
      "name": "Mihail Binev",
      "homepage": "http://kodeart.com"
    }
  ],
  "require": {
    "php": "^7.1.4",
    "psr/http-message": "~1"
  },
  "autoload": {
    "psr-4": {
      "Koded\\": ""
    },
    "files": [
      "functions.php"
    ],
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "satooshi/php-coveralls": "~1"
  },
  "autoload-dev": {
    "files": [
      "functions-dev.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev"
    }
  }
}
